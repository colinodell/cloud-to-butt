{
    "name": "colinodell/cloud-to-butt",
    "type": "library",
    "description": "Provides PSR-7 middleware for renaming \"Cloud\" to \"Butt\" in response bodies",
    "keywords": [
        "cloud",
        "butt",
        "psr-7",
        "middleware"
    ],
    "homepage": "https://github.com/colinodell/cloud-to-butt",
    "license": "MIT",
    "authors": [
        {
            "name": "Colin O'Dell",
            "email": "colinodell@gmail.com",
            "homepage": "https://www.colinodell.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ColinODell\\CloudToButt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ColinODell\\CloudToButt\\Tests\\": "tests"
        }
    },
    "require": {
        "psr/http-message": "^1.0",
        "guzzlehttp/psr7": "1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
